
public class ee{
    public static void main(String[] args){
        boolean isStudent= true;
        boolean isSenior = true;
        double price = 12.99;
        if(isStudent){
            if(isSenior){
                System.out.print("you get a senior discount 20% and student discount of 10% ");
                price *= 0.7;
            }
            else{
                System.out.print("you get a student discount of 10%");
                price *=0.9;

            }

        }
        else{
            price *=1;

        }
        System.out.printf("your final price is%.2f",price);
    }
}
