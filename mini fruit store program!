
import java.util.Random;
import java.util.Scanner;
import java.util.Arrays;
public class newfile{
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
         int n;
         System.out.print("enter the number of fruits u want to buy :");
         n = scanner.nextInt();
         scanner.nextLine();

        String[] fruitsForSale = new String[n];

        for(int i = 0 ; i<n;i++){
            System.out.print("enter the fruit u want to buy :");
            fruitsForSale[i]= scanner.nextLine();

        }
        System.out.print("The fruits u bought are : ");
        for(int j=0; j<n; j++){
            System.out.print(fruitsForSale[j] + " ");
        }

        scanner.close();


    }


}
